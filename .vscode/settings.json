        "editor.lineHighlightBackground": "#d0ff0000",
        // "editor.wordHighlightBorder": "#6f00ff",
        "editor.selectionForeground": "#ff0000",

        "list.focusForeground": "#7a05e7",
        "editor.background": "#021011a6"
    },
    "[Monokai]": {
        "sideBar.background": "#7a05e7"
